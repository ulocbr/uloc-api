{
  "name": "ulocbr/uloc-api",
  "description": "A powerful API with the basics that every web application needs",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Tiago Felipe",
      "email": "tiago@tiagofelipe.com"
    }
  ],
  "require": {
    "php": "^7.3.0",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/http-kernel": "^5.0",
    "lcobucci/jwt": "^3.2",
    "namshi/jose": "^7.2",
    "symfony/security-bundle": "^5.0",
    "doctrine/doctrine-bundle": "^2.0",
    "symfony/form": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "symfony/phpunit-bridge": "^5.0",
    "symfony/browser-kit": "^5.0",
    "symfony/console": "^5.0",
    "symfony/dom-crawler": "^5.0",
    "symfony/var-dumper": "^5.0",
    "symfony/yaml": "^5.0",
    "guzzlehttp/psr7": "^1.4",
    "php-http/guzzle6-adapter": "^1.1",
    "doctrine/doctrine-fixtures-bundle": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Uloc\\ApiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uloc\\ApiBundle\\Tests\\": "tests/"
    }
  }
}
